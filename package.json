{
  "name": "anon-texts-num-check-lambda",
  "description": "Lambda to check if provided ph# is a valid mobile phone number & record it as so",
  "version": "0.1.0",
  "devDependencies": {},
  "dependencies": {
    "aws-sdk": "^2.81.0",
    "lodash": "^4.17.4",
    "twilio": "^3.4.0"
  },
  "scripts": {
    "test:twilio": "sls invoke -f checkNumber -p event.json",
    "test:local:mobile": "sls invoke local -f checkNumber -p mobileTest.json",
    "test:local:landline": "sls invoke local -f checkNumber -p landlineTest.json",
    "logs:twilio": "sls logs -f checkNumber"
  },
